class Solution {
public:
    vector<int> arrayChange(vector<int>& nums, vector<vector<int>>& op) {
        unordered_map<int,int> mpp;
        for(int i = 0; i< nums.size(); i++){
            mpp[nums[i]] = i;
        }
        for(int i = 0 ; i< op.size(); i++){
            int idx = mpp[op[i][0]];
            nums[idx] =op[i][1];
            mpp[op[i][1]] = idx;
        }
        return nums;

  }
};
