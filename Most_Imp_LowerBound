class Solution {
public: 

    vector<int> successfulPairs(vector<int>& spells, vector<int>& potions, long long success) {
        int n = spells.size();
        int m = potions.size();
        long long index = 0;
        vector<int> ans(n, 0);

        sort(potions.begin(), potions.end());
        for(int i = 0; i< n; i++){
            long long temp = ceil((double)success/spells[i]);
            auto it = lower_bound(potions.begin(), potions.end(), temp) - potions.begin();
            cout << it << endl;
            ans[i] = m-it;
        }

        return ans;
    }
};
