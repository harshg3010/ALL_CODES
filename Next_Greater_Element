
// Use stack to keep count of next greate element form running the loop from right side of array.

#include <iostream>
#include<bits/stdc++.h>
using namespace std;

int main() {
    // Write C++ code here
    
    vector<int> arr = {6, 8, 0, 1, 3};
        int n = arr.size(); 
    vector<int> ans;
    stack<int> st;
    for(int i = n-1; i >= 0; i--){
        while(!st.empty() && st.top() <= arr[i]){
        st.pop();
    }
    if(st.empty()) ans.push_back(-1);
    else ans.push_back(st.top());
    st.push(arr[i]);
    }
    
    cout << "Printintg the next greater element :" << endl;
    for(int i =0; i< n; i++){
        cout << ans[i] << " ";
    }
    return 0;
}
